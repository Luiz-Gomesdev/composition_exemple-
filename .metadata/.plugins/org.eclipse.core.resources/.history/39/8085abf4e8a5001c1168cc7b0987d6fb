package entities;

import java.util.Date;

public class HourContract {
	
	private Date date;
	private double valuePerHour;
	private Integer hours;
	
	public HourContract() {
	}
	
	public HourContract(Date date, double valuePerHour, Integer hours) {
		super();
		this.date = date;
		this.valuePerHour = valuePerHour;
		this.hours = hours;
	}
	
	
}
